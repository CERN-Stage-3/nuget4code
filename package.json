{
	"name": "nuget4code",
	"displayName": "Nuget4Code",
	"description": "manage nuget for aspnet5",
	"version": "0.0.1",
	"publisher": "fforjan",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.nuGet"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.nuGet",
			"title": "NuGet Display packages"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
		"test": "node ./node_modules/vscode/bin/compile -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}