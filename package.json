{
	"name": "nuget4code",
	"displayName": "Nuget4Code",
	"description": "manage nuget for aspnet5",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/fforjan/nuget4code/"
	},
	"bugs": {
		"url": "https://github.com/fforjan/nuget4code//issues"
	},
	"author": {
		"name": "Frederic Forjan",
 		"email": "fforjan@free.fr",
		"url": "github.com/fforjan"
	},
	"publisher": "fforjan",
	
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.nuGet.remove",
		"onCommand:extension.nuGet.query"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.nuGet.remove",
			"title": "Nuget: Remove a package from the project.info"
		},
		{
			"command": "extension.nuGet.query",
			"title": "Nuget: query"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
		"test": "node ./node_modules/vscode/bin/compile -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}